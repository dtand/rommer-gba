{
  "game": "Metabots Rokusho",
  "keys": {
    "A": "x",
    "B": "z",
    "L": "w",
    "R": "e",
    "Start": "enter",
    "Select": "space",
    "Up": "up",
    "Down": "down",
    "Left": "left",
    "Right": "right"
  },
    "semantics": [{
      "context": "overworld",
      "A": ["talk", "confirm", "run"],
      "B": ["cancel", "open_menu"],
      "L": ["switch_tabs_left"],
      "R": ["switch_tabs_right"],
      "Start": ["start"],
      "Select": [],
      "Up": ["move_up"],
      "Down": ["move_down"],
      "Left": ["move_left"],
      "Right": ["move_right"]
    },{
      "context": "robattle",
      "A": ["attack", "confirm"],
      "B": ["defend", "cancel"],
      "L": [],
      "R": [],
      "Start": ["begin_robattle"],
      "Select": [],
      "Up": ["use_head"],
      "Down": ["use_medaforce"],
      "Left": ["use_right_arm", "scroll_options_left"],
      "Right": ["use_left_arm", "scroll_options_right"]
    }],
  "poll_interval_ms": 2000,
  "log_path": "../logs/metabots_rokusho_actions.log",
  "reward_module": "reward/metabots_rokusho_reward.py",
  "memory_watch": [
    "*"
  ]
}